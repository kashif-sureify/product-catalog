{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "types": ["vitest/globals", "vite/client"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true
  },
  "include": ["src", "tests", "vitest.config.ts"]

}
